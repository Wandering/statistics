<div id="setting_proportion" class="form-horizontal">
    <div class="control-group">
        <label class="control-label" for="">拿货代理商:</label>
        <div class="controls">
             售价 * (1 - 一级奖励比例 - 二级奖励比例)
        </div>
        <p class="form-error"></p>
    </div>
    <div class="control-group">
        <label class="control-label" for="proportion-first">一级奖励:</label>
        <div class="controls">
            售价 *  <input class="form-control form-w100" type="text" id="proportion-first" placeholder="一级奖励比例" />%
        </div>
        <p class="form-error"></p>
    </div>
    <div class="control-group">
        <label class="control-label" for="proportion-second">二级奖励:</label>
        <div class="controls">
            售价 *  <input class="form-control form-w100" type="text" id="proportion-second" placeholder="一级奖励比例" />%
        </div>
        <p class="form-error"></p>
    </div>
    <div id="model_error_msg"></div>
</div>
